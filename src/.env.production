# Server
NODE_ENV=production
PORT=80
HOST=0.0.0.0

# Database
MONGODB_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/brainScannerDB
DB_NAME=brainScannerDB

# Authentication
JWT_SECRET=your_production_super_secret_key_change_this
JWT_EXPIRES_IN=24h

# Frontend URL (CORS)
FRONTEND_URL=https://your-frontend-domain.com

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log

# Rate Limiting
RATE_LIMIT_MAX=50

# Test Services
TEST_SERVICES_BASE_URL=https://test-services.your-domain.com