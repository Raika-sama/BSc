{
    "name": "bsc-project",
    "version": "1.0.0",
    "description": "Backend sistema Brain Scanner per gestione scolastica",
    "main": "src/app.js",
    "scripts": {
        "start": "node src/app.js",
        "dev": "nodemon src/app.js",
        "migrate:create": "migrate-mongo create",
        "migrate": "node src/database/migrations/initial-setup.js",
        "migrate:up": "migrate-mongo up",
        "migrate:down": "migrate-mongo down",
        "migrate:status": "migrate-mongo status",
        "test:db": "node src/scripts/testConnection.js",
        "test": "jest --runInBand --detectOpenHandles --forceExit",
        "test:watch": "jest --watch --runInBand"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^6.3.1",
        "@mui/material": "^6.3.1",
        "@mui/x-data-grid": "^7.23.5",
        "axios": "^1.7.9",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-rate-limit": "^7.1.1",
        "express-validator": "^7.0.1",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^7.8.3",
        "morgan": "^1.10.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-router-dom": "^7.1.1",
        "react-scripts": "^5.0.1",
        "winston": "^3.17.0",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "cross-env": "^7.0.3",
        "jest": "^29.7.0",
        "migrate-mongo": "^11.0.0",
        "mongodb-memory-server": "^10.1.3",
        "mongoose-migrate-2": "^0.2.5",
        "nodemon": "^3.1.9",
        "supertest": "^6.3.4"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "jest": {
        "testTimeout": 10000,
        "verbose": true,
        "testEnvironment": "node",
        "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
        "detectOpenHandles": true,
        "testPathIgnorePatterns": [
            "/node_modules/"
        ],
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "collectCoverage": true,
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/tests/"
        ],
        "forceExit": true,
        "clearMocks": true,
        "restoreMocks": true
    },
    "author": "Raika-sama",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Raika-sama/BSc.git"
    },
    "keywords": [],
    "type": "commonjs",
    "bugs": {
        "url": "https://github.com/Raika-sama/BSc/issues"
    },
    "homepage": "https://github.com/Raika-sama/BSc#readme"
}
